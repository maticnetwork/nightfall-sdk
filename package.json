{
  "name": "nightfall-sdk",
  "version": "0.2.1",
  "description": "SDK for interacting with Polygon Nightfall",
  "keywords": [
    "nightfall",
    "sdk",
    "nightfall_3",
    "nf3"
  ],
  "main": "dist/cjs/libs/user/index.js",
  "module": "dist/libs/user/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maticnetwork/nightfall-sdk.git"
  },
  "author": "Polygon Nightfall",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.27.2",
    "bip39": "^3.0.4",
    "dotenv": "^16.0.1",
    "events": "^3.3.0",
    "joi": "^17.6.0",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "queue": "^6.0.2",
    "reconnecting-websocket": "^4.4.0",
    "web3": "^1.7.3",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/events": "^3.0.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^17.0.40",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-jest": "^28.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "rm -rf /dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS -outDir dist/cjs",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --check .",
    "prepare": "husky install",
    "test:ganache": "DOTENV_CONFIG_PATH=./examples/scripts/.env.ganache jest --setupFiles dotenv/config",
    "test:goerli": "DOTENV_CONFIG_PATH=./examples/scripts/.env.goerli jest --setupFiles dotenv/config",
    "eg:ganache:deposit": "ts-node -r dotenv/config examples/scripts/txDeposit.ts dotenv_config_path=./examples/scripts/.env.ganache dotenv_config_debug=true",
    "eg:ganache:transfer": "ts-node -r dotenv/config examples/scripts/txTransfer.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawal.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:finalise-withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawalFinalise.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:balances": "ts-node -r dotenv/config examples/scripts/balances.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:export-commitments": "ts-node -r dotenv/config examples/scripts/exportCommitments.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:import-commitments": "ts-node -r dotenv/config examples/scripts/importCommitments.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:deposit-erc721": "ts-node -r dotenv/config examples/scripts/erc721Deposit.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:transfer-erc721": "ts-node -r dotenv/config examples/scripts/erc721Transfer.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:withdrawal-erc721": "ts-node -r dotenv/config examples/scripts/erc721Withdrawal.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:deposit-erc1155": "ts-node -r dotenv/config examples/scripts/erc1155Deposit.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:transfer-erc1155": "ts-node -r dotenv/config examples/scripts/erc1155Transfer.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:withdrawal-erc1155": "ts-node -r dotenv/config examples/scripts/erc1155Withdrawal.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:goerli:deposit": "ts-node -r dotenv/config examples/scripts/txDeposit.ts dotenv_config_path=./examples/scripts/.env.goerli dotenv_config_debug=true",
    "eg:goerli:transfer": "ts-node -r dotenv/config examples/scripts/txTransfer.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawal.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:finalise-withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawalFinalise.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:balances": "ts-node -r dotenv/config examples/scripts/balances.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:export-commitments": "ts-node -r dotenv/config examples/scripts/exportCommitments.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:import-commitments": "ts-node -r dotenv/config examples/scripts/importCommitments.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:deposit-erc721": "ts-node -r dotenv/config examples/scripts/erc721Deposit.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:transfer-erc721": "ts-node -r dotenv/config examples/scripts/erc721Transfer.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:withdrawal-erc721": "ts-node -r dotenv/config examples/scripts/erc721Withdrawal.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:deposit-erc1155": "ts-node -r dotenv/config examples/scripts/erc1155Deposit.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:transfer-erc1155": "ts-node -r dotenv/config examples/scripts/erc1155Transfer.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:withdrawal-erc1155": "ts-node -r dotenv/config examples/scripts/erc1155Withdrawal.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:web-app": "npm run build && cd examples/webApp/ && npm run serve",
    "eg:react-web-app": "npm run build && cd examples/web-app/ && npm start"
  }
}
